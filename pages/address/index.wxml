<view class="container m-b6 p-bottom">
  <view class="common-header-xian"></view>
  <custom-ul class="addres-list">
    <custom-li>
      <view class="address-header b-f m-top18">
        <view class="flow-header-left pl-12">
          <view class="flow-checkout-admin">GG
            <text>18200000000</text>
          </view>
          <view class="flow-checkout-address">
            江苏省 南京市 江宁区 胜太路123号
          </view>
        </view>
      </view>
      <view class="address-list dis-flex b-f">
        <view class="list address-left">
          <radio-group bindchange="setDefault" class="radio-group">
            <label class="radio">
              <radio checked="checked" value="1"></radio>
              <text class="cont">默认</text>
              <!-- <text class="cont">选择</text> -->
            </label>
          </radio-group>
        </view>
        <view class="list address-right dis-flex">
          <view bindtap="editAddress" data-id="1">
            <text class="iconfont icon-edit"></text>
            <text class="title">编辑</text>
          </view>
          <view bindtap="removeAddress" data-id="1">
            <text class="iconfont icon-lajixiang"></text>
            <text class="title">删除</text>
          </view>
        </view>
      </view>
    </custom-li>
  </custom-ul>
